    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8083",
            "webRoot": "${workspaceFolder}/src",
            "sourceMapPathOverrides": {
                "webpack://${workspaceFolderBasename}/./src/*.vue": "${webRoot}/*.vue",
                "webpack://${workspaceFolderBasename}/./src/*.js": "${webRoot}/*.js",
            },
        }
    ]
}